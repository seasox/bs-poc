    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'bs-poc'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=bs-poc",
                    "--package=bs-poc"
                ],
                "filter": {
                    "name": "bs-poc",
                    "kind": "bin"
                }
            },
            "args": [
                "--config=esprimo-d757_i5-6400_gskill-F4-2133C15-16GIS.json",
                "--load-json=fuzz-summary.json",
                "--pattern=fe557207-465b-4cc4-8979-b9646bdca83b",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                ],
                "filter": {
                    "name": "bs-poc",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}